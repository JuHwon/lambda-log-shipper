{
  "name": "lambda-log-shipper",
  "version": "1.0.0",
  "description": "Serverless webpack example using Typescript",
  "main": "handler.js",
  "scripts": {
    "build": "webpack --config ./webpack.config.js",
    "package": "npm run build && sam package --template-file template.yml --output-template-file packaged.yml --s3-bucket lambda-log-shipper-deploy",
    "publish": "sam publish --template packaged.yml --region eu-central-1"
  },
  "dependencies": {
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.17",
    "@types/lodash": "^4.14.123",
    "@types/node": "^8.10.45",
    "aws-sdk": "^2.437.0",
    "prettier": "^1.16.4",
    "source-map-support": "^0.5.6",
    "ts-loader": "^4.2.0",
    "typescript": "^2.9.2",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.3.0",
    "yaml-cfn": "^0.2.0"
  },
  "license": "MIT"
}
